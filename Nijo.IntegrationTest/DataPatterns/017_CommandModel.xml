<?xml version="1.0" encoding="utf-8" ?>

<自動テストで作成されたプロジェクト>

  <!--
    想定シナリオ
    1. 外部システムから連携されたCSVを取り込む
    2. このシステムのデータ構造に従って取り込んだデータの変換を行う
    3. 変換後のデータを登録する
  -->

  <従業員 Type="data-model" GenerateDefaultQueryModel="True">
    <内部ID Type="word" IsKey="True" />
    <従業員コード Type="word" />
    <名前 Type="word" />
    <区分 Type="従業員区分" />
  </従業員>

  <従業員区分 Type="enum">
    <ベテラン />
    <ビギナー />
  </従業員区分>

  <従業員データ一括取り込み Type="command">
    <取込ファイル Type="word" />
    <!-- <取込ファイル Type="file" /> -->

    <処理範囲 Type="variation">
      <特定データのみ Type="variation-item:1">
        <従業員 Type="ref-to:従業員" />
      </特定データのみ>
      <全部更新 Type="variation-item:2" />
    </処理範囲>
  </従業員データ一括取り込み>
</自動テストで作成されたプロジェクト>
